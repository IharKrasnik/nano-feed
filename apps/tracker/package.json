{
  "name": "tracker",
  "version": "1.0.0",
  "description": "Waveloop tracker",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "rollup --environment BUILD:dev --config config/rollup.config.dev.js",
    "build": "rollup --config config/rollup.config.prod.js",
    "test": "eslint --ext js ./src",
    "test:full": "npm run -s test:build && npm run -s test:run",
    "test:build": "rollup --config config/rollup.config.test.js",
    "test:run": "karma start"
  },
  "author": "Paralect",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@rollup/plugin-replace": "^2.3.3",
    "fingerprintjs2": "^2.1.0",
    "husky": "4.2.5",
    "jasmine": "3.6.4",
    "jasmine-core": "3.6.0",
    "karma": "^5.0.9",
    "karma-chrome-launcher": "^3.1.0",
    "karma-detect-browsers": "^2.3.3",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.3.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^3.3.1",
    "karma-opera-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-safaritechpreview-launcher": "^2.0.2",
    "rollup": "^2.13.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^6.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  }
