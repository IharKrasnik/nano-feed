{
	"name": "momentum-wave",
	"version": "0.0.1",
	"scripts": {
		"dev": "npm run tracker:dev & vite dev",
		"build": "vite build & npm run tracker:build",
		"tracker:dev": "rollup -i tracker/src/index.js --environment BUILD:dev --config tracker/config/rollup.config.dev.js",
		"tracker:build": "rollup -i tracker/src/index.js --config tracker/config/rollup.config.prod.js",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@resvg/resvg-js": "^2.1.0",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@types/cookie": "^0.5.1",
		"autoprefixer": "^10.4.13",
		"postcss": "^8.4.20",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"satori": "^0.0.45",
		"satori-html": "^0.3.2",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"tailwindcss": "^3.2.4",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@babel/core": "^7.10.2",
		"@babel/preset-env": "^7.10.2",
		"@rollup/plugin-replace": "^2.3.3",
		"@zerodevx/svelte-toast": "1.0.0-rc.1",
		"axios": "^1.2.1",
		"dayjs": "^1.11.7",
		"fingerprintjs2": "^2.1.0",
		"lodash": "^4.17.21",
		"moment": "^2.29.4",
		"rollup": "^2.13.1",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-eslint": "^7.0.0",
		"rollup-plugin-multi-entry": "^2.1.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-terser": "^6.1.0",
		"socket.io-client": "^4.6.0",
		"svelte-tiny-linked-charts": "^1.1.5"
	}
}
